{
  "functions": {
    "api/chat-stream-enhanced.js": {
      "maxDuration": 60
    },
    "api/chat-auto.js": {
      "maxDuration": 60
    },
    "api/thinking-chain.js": {
      "maxDuration": 60
    },
    "api/embeddings-updated.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/check-env",
      "destination": "/api/system?action=check-env"
    },
    {
      "source": "/api/supabase-init",
      "destination": "/api/system?action=supabase-status"
    },
    {
      "source": "/api/test",
      "destination": "/api/system?action=health"
    }
  ]
}
